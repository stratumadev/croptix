{
    "manifest_version": 2,
    "name": "CrOptix",
    "version": "1.1",
    "description": "Crunchyroll CBR Stream forcer",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "storage",
        "*://www.crunchyroll.com/*"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    }
}
